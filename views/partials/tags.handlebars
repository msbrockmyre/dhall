<div class="panel panel-danger">
    <div class="panel-heading">
        <h3 class="panel-title" style="font-family: 'Comic Sans MS', cursive, sans-serif;">
            Tags
        </h3>
    </div>
    <div class="panel-body" style="font-family: 'Comic Sans MS', cursive, sans-serif;">
        <p class="panel-body" style="font-family: 'Comic Sans MS', cursive, sans-serif;">Dessert</p>
        {{#each sidebar.tags}}
            <div class="col-md-4 text-center" style="padding-bottom: .5em;"><a href="/images/{{uniqueId}}"><img src="/public/upload/{{filename}}" style="width: 75px; height: 75px;" class="img-thumbnail"></a></div>
        {{/each}}
        <br><br>
        <p class="panel-body" style="font-family: 'Comic Sans MS', cursive, sans-serif;">Breakfast</p>
        {{#each sidebar.breakfast}}
            <div class="col-md-4 text-center" style="padding-bottom: .5em;"><a href="/images/{{uniqueId}}"><img src="/public/upload/{{filename}}" style="width: 75px; height: 75px;" class="img-thumbnail"></a></div>
        {{/each}}
        
    </div>
</div>
